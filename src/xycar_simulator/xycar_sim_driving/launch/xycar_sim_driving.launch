<launch>
  <node name="simulator"  pkg="xycar_sim_driving" type="main.py" output="screen">
    <param name="map_name" value="square"/>
    <param name="range_sensor" value="ultrasonic"/> <!-- ultrasonic, lidar -->
    <param name="drive_mode" value="keyboard"/> <!-- ros, keyboard -->
  </node>
  <node name="driver" pkg="xycar_sim_driving" type="rule_driver.py" output="screen"/>
</launch>
