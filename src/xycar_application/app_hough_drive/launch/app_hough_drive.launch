<launch>

  <!-- Motor node -->
  <include file="$(find xycar_motor)/launch/xycar_motor.launch" />
  
  <!-- USB Camera node -->
  <include file="$(find usb_cam)/launch/xycar_cam.launch" />
  
  <!-- Ultrasonic node -->
  <include file="$(find xycar_ultrasonic)/launch/xycar_ultrasonic.launch" />

  <!-- My Driver node -->
  <node name="h_driver" pkg="app_hough_drive" type="app_hough_drive" output="screen">
    <param name="speed" value="12" />
    <param name="distance" value="10" />
  </node>

</launch>
